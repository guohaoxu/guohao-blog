<%- include header %>
<div class="row">
  <div class="col-sm-6 col-sm-offset-3">
    <form method="post" action="">
      <div class="panel panel-default">
        <div class="panel-heading">登录</div>
        <div class="panel-body">
          <div class="form-group">
            <label for="logName">用户名：</label>
            <input type="text" name="username" class="form-control" id="logName" placeholder="用户名">
          </div>
          <div class="form-group">
            <label for="logPw">密码：</label>
            <input type="password" name="password" class="form-control" id="logPw" placeholder="密码">
          </div>
        </div>
        <div class="panel-footer"><button type="submit" class="btn btn-default">登录</button></div>
      </div>
    </form>
  </div>
</div>
<h2><a href="/login/github">使用github登陆</a></h2>
<%- include footer %>
